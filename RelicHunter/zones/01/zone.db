[info]
Name=Tutorial
NumberOfRooms=3
Active=true
CanModify=no
GameDays=6


==============================================================
The weather is controlled in this file for the entire zone.
==============================================================
[weather]
current=calm
list=calm.gloomy
boost=.25

==============================================================
The moon phases are controlled in this file as well.  Please
do not remove or modify the Moon Phase section as it will cause
errors.
==============================================================
[MoonPhase]
CurrentMoonPhase=New
CurrentMoonTurn=1

[MoonPhaseTime]
full=3
new=1
gibbous=5
quarter=4
crescent=2
blood=1
BloodMoonChance=10

==============================================================
The "Time of Day" is controlled in this file. Please do not
remove or modify the time of day section as it will cause
errors.
==============================================================
[TimeOfDay]
CurrentTimeOfDay=Evening
CurrentTimeOfDayTurn=2

[TimeOfDayTime]
morning=2
noon=2
evening=2
night=2


==============================================================
ROOMS go below this line
==============================================================

[DefaultRoom]
Name=Field
Desc=You're in a field. Nothing spectacular to see here.
Shop=false
ShopName=
WarmthNeeded=0
UnderWater=false
Inside=false
Trees=0
CanDig=false


[0:0:5]
Name=The Void
Desc=You open your eyes and find yourself in a bright white area. A loud voice echos in your mind.  "You are without form. Your first order of business is to choose a race that you identify with. Use go race (such as go human).  Valid races are: Human, Elf, CreatureOfScience"  You may also use !help racename for more information on the races.

; Exits
ExitList=Human.Elf.CreatureOfScience
Human=0:0:4
Elf=0:0:4
CreatureOfScience=0:0:4

; Exit Actions
HumanAction=/.echo -q $setrace($1,Human)
ElfAction=/.echo -q $setrace($1,Elf) | $skill.add($1, NightVision, 1)
CreatureOfScienceAction=/.echo -q $setrace($1,ScienceCreation) | $skill.add($1, WaterBreathing, 1)


; Room Flags
Shop=false
ShopName=
WarmthNeeded=-100
UnderWater=false
Inside=true
zone=void
trees=0
CanDig=false

; Room Items
items=



[0:0:4]
Name=The Void
Desc="Very good $get.name($1) the $race($1) $+ .  Now you must choose a gender for yourself. Valid genders are male, female, or other"

; Exits
